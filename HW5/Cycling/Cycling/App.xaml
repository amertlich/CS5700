             StartupUri="MainWindow.xaml"
             xmlns:vm="clr-namespace:Cycling.ViewModels">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/SharedStyles.xaml" />
